6a03f14253e68cc9bbf24ab8d0fc8b61|file:///Users/koji.yoshimura/work/socrates-env/repos/socrates-bff/src/data_sources/socrates-notification/SocratesNotificationQueryNotification.ts|1|1|V|typescript
	import { DataSource } from 'apollo-datasource'
	import { credentials } from "grpc"
	import { toCommonError } from "../../mappers/bsdCommonMap"
	import { paginateInfoParser } from "../parser/PaginateInfoParser"
	import { QueryNotificationTsClient } from '../../../generated/client/query_notification_pb_client'
	import {
	  NotificationsRequest,
	  NotificationsResponse
	} from "../../../generated/useCase/query_notification_pb_usecase"
	import { PaginateOption } from "../../../generated/useCase/paginate_option_pb_usecase"
	import {
	  IQueryNotification,
	  NotificationsArgs,
	  NotificationsResult,
	  NotificationsResultEnum,
	  NotificationData
	} from '../IQueryNotification'
	
	export class SocratesNotificationQueryNotification extends DataSource implements IQueryNotification {
	  private client: QueryNotificationTsClient
	
	  constructor({ host }: { host: string }) {
	    super();
	    this.client = new QueryNotificationTsClient(
	      host,
	      credentials.createInsecure()
	    )
	  }
	
	  async notifications(args: NotificationsArgs): Promise<NotificationsResult> {
	    const paginateOption: PaginateOption = {
	      page: 1,
	      size: 20,
	      withPaginateInfo: false,
	      paginateRange: 0
	    }
	    const request = {
	      userId: args.userId,
	      paginateOption: paginateOption
	    }
	    const response: NotificationsResponse = await this.client.notifications(request)
	    return {
	      result: response.result.toString() as NotificationsResultEnum,
	      errorsList: response.errorsList.map(toCommonError),
	      notificationList: response.notificationList as NotificationData[],
	      paginateInfo: paginateInfoParser(response.paginateInfo),
	      unreadCount: response.unreadCount
	    }
	  }
	}
