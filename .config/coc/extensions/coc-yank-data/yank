	import { Error } from "./Common"
	
	export interface ICommandMyTag {
	  addMyTag(args: AddMyTagArgs): Promise<AddMyTagResult>
	  removeMyTag(args: RemoveMyTagArgs): Promise<RemoveMyTagResult>
	}
	
	export interface AddMyTagArgs {
	  userId: string
	  tagIdList: string[]
	}
	
	export interface AddMyTagResult {
	  result: AddMyTagResultEnum
	  errorList: Error[]
	}
	
	export type AddMyTagResultEnum =
	  | "Unknown"
	  | "Success"
	  | "ValidationFailed"
	  | "UserNotFound"
	
	export interface RemoveMyTagArgs {
	  userId: string
	  tagId: string
	}
	
	export interface RemoveMyTagResult {
	  result: RemoveMyTagResultEnum
	  errorList: Error[]
	}
	
	export type RemoveMyTagResultEnum =
	  | "Unknown"
	  | "Success"
	  | "ValidationFailed"
	  | "MyTagNotFound"
	  | "UserNotFound"
