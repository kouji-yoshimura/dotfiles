e95dc036ed93469d886d2f81b6e6505e|file:///Users/koji.yoshimura/work/socrates-env/repos/socrates-bff/src/resolvers/Mutation/AcceptAnswerResolver.ts|1|1|V|typescript
	import { NexusGenTypes } from '../../nexus-typegen'
	import { AcceptAnswerResultEnum } from '../../data_sources/ICommandAcceptAnswer'
	
	export const AcceptAnswerResolver = async (
	  _: NexusGenTypes["rootTypes"]["Mutation"],
	  args: NexusGenTypes["argTypes"]["Mutation"]["acceptAnswer"],
	  ctx: NexusGenTypes["context"]
	): Promise<NexusGenTypes["fieldTypes"]["Mutation"]["acceptAnswer"]> => {
	  if (!ctx.user_id) {
	    throw new Error("Could not find user")
	  }
	
	  const res = await ctx.dataSources.commandAcceptAnswer.acceptAnswer({
	    userId: ctx.user_id,
	    answerId: args.params.answer_id
	  })
	
	  switch (res.result as AcceptAnswerResultEnum) {
	    case "Success":
	      return {
	        __typename: "AcceptAnswerResultSuccess",
	        message: ""
	      }
	    case "ValidationFailed":
	      return {
	        __typename: "AcceptAnswerValidationFailedError",
	        message: ""
	      }
	    case "InvalidArgument":
	      return {
	        __typename: "AcceptAnswerAlreadyAcceptedError",
	        message: ""
	      }
	    default:
	      throw new Error("Unexpected Error")
	  }
	}
	
