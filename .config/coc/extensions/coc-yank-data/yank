a44ec6698c9a79b215a6a8e7e1011ce3|file:///Users/koji.yoshimura/work/socrates-env/repos/socrates-notification/src/domain/notification/UseCases/CommandNotification/ReadAllNotifications/ReadAllNotificationsGrpcController.ts|1|1|V|typescript
	import { Metadata } from "grpc"
	import { inject, injectable } from "tsyringe"
	import {
	  BadRequestError,
	  BsdCommonErrorMapper,
	  UnexpectedError
	} from "@lv-levtech/socrates-kit-ts"
	import {
	  ReadAllNotificationsRequest,
	  ReadAllNotificationsResponse
	} from "../../../../../../generated/useCase"
	import * as _enum from "../../../../../../generated/enum/command_notification_pb_enum"
	import { ReadAllNotifications } from "./ReadAllNotifications"
	import { ReadAllNotificationsDTO } from "./ReadAllNotificationsDTO"
	
	@injectable()
	export class ReadAllNotificationsGrpcController {
	  constructor(
	    @inject(ReadAllNotifications)
	    private sendNotificationByAcceptAnswer: ReadAllNotifications
	  ) {}
	
	  async execute(
	    req: ReadAllNotificationsRequest,
	    metadata?: Metadata
	  ): Promise<ReadAllNotificationsResponse> {
	    const reqDTO = req as ReadAllNotificationsDTO
	    const res = await this.sendNotificationByAcceptAnswer.execute(reqDTO)
	
	    return res
	      .map<ReadAllNotificationsResponse>(() => {
	        return {
	          result: _enum.ReadAllNotificationsResponse.Result.Success,
	          errorList: []
	        }
	      })
	      .getOrElse((error) => {
	        const resultType = ((
	          errorTypeOf: any
	        ): _enum.ReadAllNotificationsResponse.Result => {
	          switch (errorTypeOf) {
	            case BadRequestError:
	              return _enum.ReadAllNotificationsResponse.Result.ValidationFailed
	            case UnexpectedError:
	            default:
	              throw new Error(error.errorValue().message)
	          }
	        })(error.constructor)
	
	        return {
	          result: resultType,
	          errorList: [BsdCommonErrorMapper(resultType.toString(), error)]
	        }
	      })
	  }
	}
