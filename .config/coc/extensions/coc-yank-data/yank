	import { inject, injectable } from "tsyringe"
	import { Result } from "../../../../../shared/core/Result"
	import { UnexpectedError } from "../../../../../shared/core/AppError"
	import {
	  SendNotificationByAcceptAnswerRequest,
	  SendNotificationByAcceptAnswerResponse
	} from "../../../../../../generated/useCase"
	import * as _enum from "../../../../../../generated/enum/command_notification_pb_enum"
	import { SendNotificationByAcceptAnswer } from "./SendNotificationByAcceptAnswer"
	import { SendNotificationByAcceptAnswerDTO } from "./SendNotificationByAcceptAnswerDTO"
	
	@injectable()
	export class SendNotificationByAcceptAnswerGrpcController {
	  constructor(
	    @inject(SendNotificationByAcceptAnswer)
	    private sendNotificationByAcceptAnswer: SendNotificationByAcceptAnswer
	  ) {}
	
	  async execute(
	    req: SendNotificationByAcceptAnswerRequest,
	    metadata?: Metadata
	  ): Promise<SendNotificationByAcceptAnswerResponse> {
	    const reqDTO = req as SendNotificationByAcceptAnswerDTO
	    console.log({ reqDTO })
	    const res = await this.sendNotificationByAcceptAnswer.execute(reqDTO)
	    console.log({ res })
	
	    return res
	      .map<SendNotificationByAcceptAnswerResponse>((value: Result<void>) => {
	        return {
	          result: _enum.SendNotificationByAcceptAnswerResponse.Result.Success,
	          errorList: []
	        }
	      })
	      .getOrElse((error) => {
	        const resultType = ((
	          errorTypeOf: any
	        ): _enum.SendNotificationByAcceptAnswerResponse.Result => {
	          switch (errorTypeOf) {
	            case UnexpectedError:
	            default:
	              throw new Error(error.errorValue().message)
	          }
	        })(error.constructor)
	      })
	  }
	}
